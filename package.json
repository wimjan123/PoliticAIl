{
  "name": "politicail",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Political Desktop OS Simulation - AI-driven political strategy game",
  "author": "Political AI Team",
  "license": "MIT",
  "keywords": [
    "political",
    "simulation",
    "desktop",
    "tauri",
    "react",
    "ai"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:backend": "node src/index.js",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev\"",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:up:dev": "docker-compose --profile dev-tools up -d",
    "docker:up:search": "docker-compose --profile search up -d",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "db:seed": "node scripts/seedDatabase.js",
    "health": "node scripts/healthCheck.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "test:integration": "jest --testMatch=\"**/*.integration.test.(ts|tsx)\" --runInBand",
    "test:integration:window": "jest --testMatch=\"**/window-simulation.integration.test.(ts|tsx)\" --runInBand",
    "test:integration:persistence": "jest --testMatch=\"**/data-persistence.integration.test.(ts|tsx)\" --runInBand",
    "test:integration:platform": "jest --testMatch=\"**/cross-platform.integration.test.(ts|tsx)\" --runInBand",
    "test:integration:performance": "jest --testMatch=\"**/performance.integration.test.(ts|tsx)\" --runInBand --testTimeout=60000",
    "test:integration:all": "node ./scripts/integration-test-runner.js",
    "test:performance": "jest --testMatch=\"**/*.performance.test.(ts|tsx)\" --runInBand --forceExit",
    "test:performance:check": "node ./scripts/performance-check.js",
    "test:memory": "node --expose-gc --max-old-space-size=4096 ./scripts/memory-test.js",
    "ci:install": "npm ci",
    "ci:lint": "npm run lint && npm run type-check",
    "ci:test": "npm run test:ci",
    "ci:build": "npm run build",
    "ci:security": "npm audit --audit-level high",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "validate": "npm run lint && npm run type-check && npm run test:ci",
    "clean": "rimraf dist build coverage .eslintcache",
    "clean:deps": "rimraf node_modules package-lock.json && npm install",
    "clean:tauri": "rimraf src-tauri/target",
    "clean:all": "npm run clean && npm run clean:deps && npm run clean:tauri"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.41.0",
    "@tanstack/react-query-devtools": "^4.41.0",
    "@tauri-apps/api": "^2.8.0",
    "@tauri-apps/plugin-shell": "^2.3.1",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "compression": "^1.7.4",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "mongodb": "^6.3.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.8.0",
    "redis": "^4.6.11",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2",
    "zod": "^4.1.9",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.8.4",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.51.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-testing-library": "^6.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/politicail.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/politicail/issues"
  },
  "homepage": "https://github.com/your-org/politicail#readme"
}
